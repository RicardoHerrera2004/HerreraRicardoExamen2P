<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="HerreraRicardoExamen2P.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:HerreraRicardoExamen2P"
    Shell.FlyoutBehavior="Disabled"
    Title="HerreraRicardoExamen2P">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

        <ShellContent
        Title="Chiste"
        ContentTemplate="{DataTemplate local:ChistePage}"
        Route="ChistePage" />

        <ShellContent
        Title="Infomacion"
        ContentTemplate="{DataTemplate local:InformacionPage}"
        Route="InformacionPage" />
    </FlyoutItem>

</Shell>